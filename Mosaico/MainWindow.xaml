<Window x:Class="Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mosaico"
        mc:Ignorable="d"
        Title="MainWindow" Height="675" Width="675" MinHeight="473" MinWidth="450">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="#FF8400D2"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="#FF8400D2"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Green"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="Green"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="#FFFF5200"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="#FFFF5200"></TextBlock>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="Magenta"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="Magenta"></TextBlock>
        <TextBlock Background="Black"></TextBlock>
    </DockPanel>
</Window>
